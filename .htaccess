<Files .htaccess>
order allow,deny
deny from all
</Files>

RewriteEngine on

# Check if domain is "localhost"
RewriteCond %{HTTP_HOST} ^localhost$

RewriteCond %{REQUEST_URI} !^/beta.ambatitastyfoods.com/app/
RewriteCond %{REQUEST_URI} !^/beta.ambatitastyfoods.com/lib/
RewriteCond %{REQUEST_URI} !^/beta.ambatitastyfoods.com/desktop/
RewriteCond %{REQUEST_URI} !^/beta.ambatitastyfoods.com/.env
RewriteCond %{REQUEST_URI} !^/beta.ambatitastyfoods.com/variables.json

RewriteRule ^(.*)$ /beta.ambatitastyfoods.com/app/$1 [L]

# Check if domain is not "localhost"
RewriteCond %{HTTP_HOST} !^localhost$

RewriteCond %{REQUEST_URI} !^/app/
RewriteCond %{REQUEST_URI} !^/lib/
RewriteCond %{REQUEST_URI} !^/desktop/
RewriteCond %{REQUEST_URI} !^/.env
RewriteCond %{REQUEST_URI} !^/variables.json

RewriteRule ^(.*)$ /app/$1 [L]